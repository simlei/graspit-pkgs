runtime_require_shellenv interactive "$(getmod -n catkin_ws)"

convert() {
    local handconvert="$(getmod -n handconvert)"
    (cd "$handconvert"; hereshell; convertToGraspit;)
}
convertAndShow() {
    local handconvert="$(getmod -n handconvert)"
    (cd "$handconvert"; hereshell; convertToGraspit; installGraspit; launchGraspit)
}
rebuild() {
    local catkin="$(getmod -n catkin_ws)"
    withcatkin catkin build urdf2graspit
}
testgitversion() {
    rebuild; convertAndShow;
}
